direction: column;\n  justify-content: center;\n  align-items: center;\n  width: 70px;\n  height: 70px;\n  font-size: 15px;\n  color: rgb(184, 184, 184);\n  background: none;\n  border-style: none;\n  cursor: pointer;\n}\n\n.openCartBtn img {\n  width: 90%;\n}\n\n.logo {\n  width: 20%;\n  display: flex;\n  justify-content: start;\n  align-items: center;\n  border-radius: 10px;\n  cursor: pointer;\n}\n\n.logo p {\n  color: white;\n  font-size: 30px;\n}\n\n.cartImg {\n  width: 30%;\n}\n\n/* Модалка корзины_____________ */\n.modal {\n  position: fixed;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100vh;\n  z-index: 9998;\n  background-color: rgba(0, 0, 0, 0.9);\n  display: grid;\n  align-items: center;\n  justify-content: center;\n  overflow-y: auto;\n  visibility: hidden;\n  opacity: 0;\n  transition: opacity 0.4s, visibility 0.4s;\n}\n\n.modal.open {\n  visibility: visible;\n  opacity: 1;\n}\n\n.modalBox {\n  max-width: 500px;\n  height: 600px;\n  background: white;\n  padding: 30px;\n  z-index: 1;\n  margin: 30px 15px;\n  border-radius: 10px;\n  transform: scale(0);\n  display: flex;\n  flex-direction: column;\n}\n\n.closeModalBtn {\n  position: absolute;\n  top: 10px;\n  right: 10px;\n  width: 30px;\n  height: 30px;\n  cursor: pointer;\n  border-radius: 50%;\n}\n\n.modal.open .modalBox {\n  transform: scale(1);\n}\n\n/* модалка инпута____________ */\n.modalSearch {\n  position: fixed;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100vh;\n  z-index: 9998;\n  visibility: hidden;\n  opacity: 0;\n  transition: opacity 0.4s, visibility 0.4s;\n}\n\n.modalSearch.open {\n  visibility: visible;\n  opacity: 1;\n}\n\n.modalSearchBox {\n  padding-top: 10px;\n  width: 100%;\n  height: 100vh;\n  background: white;\n  z-index: 1;\n  border-radius: 10px;\n  transform: scale(0);\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: flex-start;\n}\n\n.closeModalSearchBtn {\n  color: #cb11ab;\n  height: 50px;\n  width: 10%;\n  border: none;\n  background: white;\n  font-weight: 700;\n}\n\n.modalSearchBox input {\n  width: 80%;\n  height: 50px;\n  border: 1px solid black;\n  margin: 0;\n  background-color: rgba(0, 0, 0, 0.1);\n  border: none;\n}\n\n.modalSearch.open .modalSearchBox {\n  transform: scale(1);\n}\n\n/* слайдер___________ */\n.slider {\n  margin: 30px auto;\n  width: 90%;\n}\n\n.slider img {\n  width: 100%;\n  border-radius: 20px;\n}\n\n/* Товар_____________ */\n.bestseller {\n  width: 90%;\n}\n\n.bestseller p {\n  margin: 10px;\n  font-size: 36px;\n  font-weight: 600;\n}\n\n.cardProduct {\n  width: 100%;\n  display: grid;\n  justify-content: space-between;\n  grid-template-columns: repeat(5, minmax(150px, 261px));\n  grid-gap: 50px 30px;\n}\n\n.cardProductId {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.prodPriceBtn {\n  width: 100%;\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  align-items: center;\n}\n\n.prodPriceBtn button {\n  height: auto;\n  font-weight: 600;\n  font-size: 1vw;\n  padding: 5px 11px 6px;\n  border-radius: 6px;\n  background-color: #cb11ab;\n  color: white;\n  border: none;\n  cursor: pointer;\n}\n\n.prodPriceBtn p {\n  font-size: 2vw;\n}\n\n.prodPriceBtn:hover {\n  color: #cb11ab;\n}\n\n.cardProductId img {\n  width: 80%;\n}\n\n/* Ну тут понятно_____________ */\nfooter {\n  margin-top: 30px;\n  padding: 20px, 10px;\n  width: 100%;\n  height: 400px;\n  background: #481173;\n}\n\nfooter p {\n  margin-top: 50px;\n  color: white;\n}\n"},"lineCount":361}},"error":null,"hash":"2bb739cf25379dd679889feaaa4cdd59","cacheData":{"env":{}}}